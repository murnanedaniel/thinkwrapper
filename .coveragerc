[run]
source = app
omit =
    */tests/*
    */venv/*
    */.venv/*
    */migrations/*
    */instance/*
    app/__init__.py
    app/config.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml 